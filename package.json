{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^9.7.3",
    "http-proxy-middleware": "^0.20.0",
    "jest-fetch-mock": "^3.0.0",
    "postcss-cli": "^6.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "docs": "yarn build-styles && npx styleguidist server",
    "build-docs": "yarn build-styles && npx styleguidist build",
    "build-styles": "postcss src/styles/tailwind.css -o src/styles/index.css",
    "prebuild": "yarn build-styles",
    "prestart": "yarn build-styles"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-loader": "^8.0.6",
    "react-styleguidist": "^10.3.2",
    "tailwindcss": "^1.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/stories/"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
